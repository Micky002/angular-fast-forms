{
  "name": "frontend",
  "version": "0.0.0",
  "license": "MIT",
  "publishConfig": {
    "registry": "https://nexus.contabo-donald.codentury.com/repository/npm-codentury-hosted"
  },
  "scripts": {
    "ng": "nx",
    "postinstall": "node ./decorate-angular-cli.js && ngcc --properties es2020 browser module main",
    "build:core": "nx build core:build",
    "build:material": "nx build material:build",
    "start:material-example": "nx serve material-example",
    "lint:core": "nx lint core",
    "lint:material": "nx lint material",

    "build:auth-ui:production": "nx build auth-ui --configuration=production",
    "build:api-client:production": "nx build api-client --configuration=production",
    "build:auth-client:production": "nx build auth-client --configuration=production",
    "build:dynamic-forms:production": "nx build dynamic-forms --configuration=production",
    "build:common-ui:production": "nx build common-ui --configuration=production",
    "test": "nx test",
    "test:all:single": "npm run test:auth-ui && npm run test:auth-client && npm run test:dynamic-forms && npm run test:common-ui",
    "test:auth-ui": "nx test auth-ui",
    "test:auth-client": "nx test auth-client",
    "test:dynamic-forms": "nx test dynamic-forms",
    "test:common-ui": "nx test common-ui",
    "lint:all:single": "npm run lint:auth-ui && npm run lint:auth-client && npm run lint:dynamic-forms && npm run lint:common-ui",
    "lint:auth-ui": "nx lint auth-ui",
    "lint:auth-client": "nx lint auth-client",
    "lint:dynamic-forms": "nx lint dynamic-forms",
    "lint:common-ui": "nx lint common-ui",
    "publish:api-client": "npm publish dist/libs/api-client",
    "publish:auth-client": "npm publish dist/libs/auth-client",
    "publish:dynamic-forms": "npm publish dist/libs/dynamic-forms",
    "publish:common-ui": "npm publish dist/libs/common-ui",
    "swagger:all": "npm run swagger:api-client && npm run swagger:api-client-internal",
    "swagger:api-client": "openapi-generator-cli generate -g typescript-angular -i http://localhost:8180/v3/api-docs/public -o libs/api-client/src --additional-properties ngVersion=14.0.0 --additional-properties enumPropertyNaming=UPPERCASE --additional-properties stringEnums=true --additional-properties useSingleRequestParameter=true --additional-properties serviceSuffix='' --additional-properties configurationPrefix='Auth' --additional-properties providedIn='none'",
    "swagger:api-client-internal": "openapi-generator-cli generate -g typescript-angular -i http://localhost:8180/v3/api-docs/internal -o libs/api-client-internal/src --additional-properties ngVersion=14.0.0 --additional-properties enumPropertyNaming=UPPERCASE --additional-properties stringEnums=true --additional-properties useSingleRequestParameter=true --additional-properties serviceSuffix='' --additional-properties apiModulePrefix='Internal' --additional-properties configurationPrefix='Internal'",
    "version-set": "npm version ${NEXT_NPM_VERSION} && cd libs/api-client && npm version ${NEXT_NPM_VERSION} && cd ../auth-client && npm version ${NEXT_NPM_VERSION} && cd ../dynamic-forms && npm version ${NEXT_NPM_VERSION} && cd ../common-ui && npm version ${NEXT_NPM_VERSION}",
    "graph": "nx graph",
    "compile-scss": "sass libs/common-ui/src/styles/bundled/bundled.scss --load-path=./node_modules libs/common-ui/src/styles/bundled/prebuild-bundle.css && npm run distribute-style",
    "distribute-style": "cp libs/common-ui/src/styles/bundled/prebuild-bundle.css ../service/src/main/resources/templates/prebuild-bundle.css",
    "sass": "sass --help"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "14.1.3",
    "@angular/cdk": "14.1.3",
    "@angular/common": "14.1.3",
    "@angular/compiler": "14.1.3",
    "@angular/core": "14.1.3",
    "@angular/forms": "14.1.3",
    "@angular/material": "14.1.3",
    "@angular/material-luxon-adapter": "14.1.3",
    "@angular/platform-browser": "14.1.3",
    "@angular/platform-browser-dynamic": "14.1.3",
    "@angular/router": "14.1.3",
    "@ngneat/until-destroy": "9.2.1",
    "@nrwl/angular": "14.5.9",
    "date-fns": "2.29.2",
    "luxon": "3.0.1",
    "rxjs": "7.5.6",
    "tslib": "~2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "14.1.3",
    "@angular-eslint/eslint-plugin": "14.0.2",
    "@angular-eslint/eslint-plugin-template": "14.0.2",
    "@angular-eslint/template-parser": "14.0.2",
    "@angular/cli": "14.1.3",
    "@angular/compiler-cli": "14.1.3",
    "@angular/language-service": "14.1.3",
    "@nrwl/cli": "14.5.9",
    "@nrwl/cypress": "14.5.9",
    "@nrwl/eslint-plugin-nx": "14.5.9",
    "@nrwl/jest": "14.5.9",
    "@nrwl/linter": "14.5.9",
    "@nrwl/storybook": "^14.5.8",
    "@nrwl/workspace": "14.5.9",
    "@storybook/addon-essentials": "~6.5.9",
    "@storybook/angular": "~6.5.9",
    "@storybook/builder-webpack5": "~6.5.9",
    "@storybook/core-server": "~6.5.9",
    "@storybook/manager-webpack5": "~6.5.9",
    "@types/jasmine": "~4.0.0",
    "@types/jest": "27.4.1",
    "@types/luxon": "~2.3.2",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "~5.30.7",
    "@typescript-eslint/parser": "~5.30.7",
    "cypress": "~9.1.0",
    "eslint": "~8.20.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-cypress": "~2.12.1",
    "jasmine-core": "4.2.0",
    "jasmine-spec-reporter": "~7.0.0",
    "jest": "27.5.1",
    "jest-preset-angular": "~11.1.2",
    "ng-packagr": "~14.1.0",
    "nx": "14.5.9",
    "postcss": "~8.4.5",
    "postcss-import": "~14.1.0",
    "postcss-preset-env": "~7.5.0",
    "postcss-url": "~10.1.3",
    "prettier": "~2.6.2",
    "sass": "~1.54.2",
    "ts-jest": "27.1.4",
    "ts-node": "10.9.1",
    "typescript": "~4.7.2",
    "webpack": "^5.64.0"
  }
}
