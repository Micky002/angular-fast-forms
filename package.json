{
  "name": "angular-fast-forms",
  "version": "3.0.0-rc.2",
  "license": "MIT",
  "packageManager": "npm@10.2.3",
  "engines": {
    "node": "18.19.0"
  },
  "scripts": {
    "ng": "nx",
    "preinstall": "rm -rf .angular && rm -rf coverage && rm -rf dist && rm -rf tmp && nx reset",
    "postinstall": "node ./decorate-angular-cli.js",
    "build:core": "nx run core:build",
    "build:material": "nx run material:build",
    "build:all": "nx run-many --target=build --projects=core,material",
    "start:material-example": "nx serve material-example",
    "start:storybook": "nx storybook storybook",
    "pretest": "npm run lint:all",
    "test": "nx affected --all --target=test && npm run test-component:all && npm run build:all",
    "test:ci": "nx affected --all --target=test --runInBand --coverage --parallel=1 --output-style=static",
    "test:all": "nx affected --all --target=test --coverage",
    "test-component:all": "nx affected --all --target=component-test --parallel=1",
    "test-component:material-example:watch": "nx run material-example:component-test --watch",
    "test-component:material:watch": "nx run material:component-test --watch",
    "lint:all": "nx affected --all --target=lint --exclude=storybook,storybook-e2e,material-example-e2e",
    "release": "standard-version",
    "graph": "nx graph"
  },
  "dependencies": {
    "@angular/animations": "^15.2.0",
    "@angular/cdk": "^15.2.0",
    "@angular/common": "^15.2.0",
    "@angular/compiler": "^15.2.0",
    "@angular/core": "^15.2.0",
    "@angular/forms": "^15.2.0",
    "@angular/material": "^15.2.0",
    "@angular/material-luxon-adapter": "^15.2.0",
    "@angular/platform-browser": "^15.2.0",
    "@angular/platform-browser-dynamic": "^15.2.0",
    "@angular/router": "^15.2.0",
    "@fontsource/material-icons": "4.5.4",
    "@fontsource/roboto": "4.5.8",
    "@ngxs/form-plugin": "~3.8.2",
    "@ngxs/store": "~3.8.2",
    "luxon": "3.4.4",
    "rxjs": "~7.8.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.10",
    "@angular-devkit/architect": "~0.1502.10",
    "@angular-eslint/eslint-plugin": "15.0.0",
    "@angular-eslint/eslint-plugin-template": "15.0.0",
    "@angular-eslint/template-parser": "15.0.0",
    "@angular/cli": "^15.2.10",
    "@angular/compiler-cli": "^15.2.0",
    "@angular/language-service": "^15.2.9",
    "@cypress/webpack-dev-server": "^2.0.0",
    "@ngxs/devtools-plugin": "~3.8.2",
    "@nrwl/angular": "^15.9.2",
    "@nrwl/cli": "^15.9.2",
    "@nrwl/cypress": "^15.9.2",
    "@nrwl/eslint-plugin-nx": "^15.9.2",
    "@nrwl/jest": "^15.9.2",
    "@nrwl/linter": "^15.9.2",
    "@nrwl/storybook": "^15.9.2",
    "@nrwl/workspace": "^15.9.2",
    "@schematics/angular": "^15.2.0",
    "@storybook/addon-essentials": "~6.5.0",
    "@storybook/angular": "~6.5.0",
    "@storybook/builder-webpack5": "~6.5.0",
    "@storybook/core-server": "~6.5.0",
    "@storybook/manager-webpack5": "~6.5.0",
    "@types/jest": "29.4.4",
    "@types/luxon": "2.3.2",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "5.30.7",
    "@typescript-eslint/parser": "5.30.7",
    "cypress": "^12.2.0",
    "dependency-cruiser": "^12.10.0",
    "eslint": "8.34.0",
    "eslint-config-prettier": "8.6.0",
    "eslint-plugin-cypress": "2.12.1",
    "html-webpack-plugin": "^5.5.0",
    "jest": "29.4.3",
    "jest-environment-jsdom": "29.4.3",
    "jest-junit": "14.0.0",
    "jest-preset-angular": "13.0.0",
    "ng-packagr": "^15.2.0",
    "nx": "^15.9.0",
    "postcss": "8.4.21",
    "postcss-import": "14.1.0",
    "postcss-preset-env": "7.5.0",
    "postcss-url": "10.1.3",
    "prettier": "2.6.2",
    "sass": "1.54.2",
    "standard-version": "^9.5.0",
    "ts-jest": "29.0.5",
    "ts-node": "10.9.1",
    "typescript": "~4.9.4",
    "webpack": "~5.76.0"
  }
}
