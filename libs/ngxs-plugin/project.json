{
  "projectType": "library",
  "sourceRoot": "libs/ngxs-plugin/src",
  "prefix": "angular-fast-forms",
  "targets": {
    "build": {
      "executor": "@nrwl/angular:package",
      "outputs": ["dist/libs/ngxs-plugin"],
      "options": {
        "project": "libs/ngxs-plugin/ng-package.json"
      },
      "configurations": {
        "production": {
          "tsConfig": "libs/ngxs-plugin/tsconfig.lib.prod.json"
        },
        "development": {
          "tsConfig": "libs/ngxs-plugin/tsconfig.lib.json"
        }
      },
      "defaultConfiguration": "production"
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/ngxs-plugin"],
      "options": {
        "jestConfig": "libs/ngxs-plugin/jest.config.ts",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": [
          "libs/ngxs-plugin/**/*.ts",
          "libs/ngxs-plugin/**/*.html"
        ]
      }
    }
  },
  "tags": []
}
